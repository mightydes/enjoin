{
  "name": "mightydes/enjoin",
  "description": "Multi dialect ORM for PHP inspired by Sequelize.JS",
  "license": "GNU",
  "keywords": [
    "mysql",
    "postgresql",
    "postgres",
    "orm",
    "laravel",
    "object relational mapper",
    "sequelize"
  ],
  "authors": [
    {
      "name": "Lev Nazarenko",
      "email": "gwae@yandex.ru"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/cache": ">=5.1",
    "illuminate/container": ">=5.1",
    "illuminate/database": ">=5.1",
    "illuminate/redis": ">=5.1",
    "illuminate/translation": ">=5.1",
    "illuminate/validation": ">=5.1",
    "doctrine/inflector": "~1.0",
    "nesbot/carbon": "~1.0",
    "panique/pdo-debug": "0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "raveren/kint": "^1.0",
    "caouecs/laravel-lang": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Enjoin\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Models\\": [
        "test/php/models"
      ]
    }
  },
  "minimum-stability": "stable"
}
